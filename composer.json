{
    "name": "mjolnic/purephp",
    "description": "A lightweight RESTFul framework with legacy support for PHP 5.2+ . Based on expressjs",
    "keywords": ["lightweight", "framework", "restful", "express"],
    "license": "MIT",
    "authors": [
        {
            "name": "Javier Aguilar",
            "homepage": "http://mjolnic.github.io/"
        }
    ],
    "require": {
        "php": ">=5.3.3"
    },
    "autoload": {
        "psr-0": {
            "Pure": "vendor/mjolnic/purephp/",
            "": "app/src/"
        }
    },
    "config": {
        "preferred-install": "dist"
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
