{
    "name": "mjolnic/purephp",
    "description": "A lightweight RESTFul framework for PHP 5.3.3+",
    "keywords": ["lightweight", "framework", "restful", "express"],
    "license": "MIT",
    "authors": [
        {
            "name": "Javier Aguilar",
            "homepage": "http://mjolnic.github.io/"
        }
    ],
    "require": {
        "php": ">=5.3.3",
        "filp/whoops":"1.1.*",
        "monolog/monolog":"1.9.*",
        "ircmaxell/password-compat": "1.0.3",
        "gabordemooij/redbean": "3.5.*",
        "illuminate/view": "4.1.*",
        "swiftmailer/swiftmailer": "5.2.*"
    },
    "require-dev": {
        "phpunit/phpunit": "4.1.*"
    },
    "autoload": {
        "psr-0": {
            "": "app/src/"
        }
    },
    "scripts": {
        
    },
    "config": {
        "preferred-install": "dist"
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
